<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <StateSet motion:defaultState="@id/start1"/>

    <ConstraintSet android:id="@+id/start1" >

        <Constraint
            android:id="@+id/layout2"
            android:alpha="0"
            android:translationZ="-1dp"
            android:translationX="400dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" >

        </Constraint>
        <Constraint
            android:id="@+id/layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:translationX="400dp"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" />
        <Constraint
            android:id="@+id/keyboardLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="visible"
            motion:layout_constraintTop_toBottomOf="@id/layout1" />
        <Constraint
            android:id="@+id/table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView1"
            android:translationZ="-4dp"
            android:layout_marginTop="5dp"
            android:alpha="0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/state_1_1" >

        <Constraint
            android:id="@+id/layout2"
            android:alpha="0"
            android:translationZ="-1dp"
            android:translationX="800dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" >

        </Constraint>
        <Constraint
            android:id="@+id/layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" />
        <Constraint
            android:id="@+id/keyboardLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="visible"
            motion:layout_constraintTop_toBottomOf="@id/layout1" />
        <Constraint
            android:id="@+id/table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView1"
            android:translationZ="-4dp"
            android:layout_marginTop="5dp"
            android:alpha="0" />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/state_1_2" >
        <Constraint
            android:alpha="0"
            android:translationZ="-1dp"
            android:id="@+id/layout2"
            android:translationX="800dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" >

        </Constraint>
        <Constraint
            android:id="@+id/layout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" />
        <Constraint
            android:id="@+id/keyboardLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:translationZ="-4dp"
            motion:layout_constraintTop_toBottomOf="@id/layout1"
            android:alpha="0" />
        <Constraint
            android:id="@+id/button6_1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:translationZ="-4dp"
            motion:layout_constraintTop_toBottomOf="@+id/keyboardLayout1"
            android:alpha="0" />
        <Constraint
            android:id="@+id/table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView1"
            android:translationZ="4dp"
            android:layout_marginTop="5dp"
             />

        <Constraint
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/layout1"
             />
        <Constraint
            android:id="@+id/cardGraph"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            motion:layout_constraintTop_toBottomOf="@id/table"
             />
    </ConstraintSet>
    <ConstraintSet android:id="@+id/state_2_1" >
        <Constraint
            android:alpha="0"
            android:translationZ="-1dp"
            android:id="@+id/layout1"
            android:translationX="-800dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" >

        </Constraint>
        <Constraint

            android:id="@+id/layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" />
        <Constraint
            android:id="@+id/keyboardLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:visibility="visible"
            motion:layout_constraintTop_toBottomOf="@id/layout1" />
        <Constraint
            android:id="@+id/table"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView1"
            android:translationZ="-4dp"
            android:layout_marginTop="5dp"
            android:alpha="0" />

    </ConstraintSet>
    <ConstraintSet android:id="@+id/state_2_2" >
        <Constraint
            android:alpha="0"

            android:translationZ="-4dp"
            android:id="@+id/layout1"
            android:translationX="-800dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" >

        </Constraint>
        <Constraint
            android:id="@+id/layout2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/radioGroup6_1" />
        <Constraint
            android:id="@+id/keyboardLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@id/layout1"
            android:alpha="0" />
        <Constraint
            android:id="@+id/button6_1"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/keyboardLayout1"
            android:alpha="0" />
        <Constraint
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/layout1"
            />
        <Constraint
            android:id="@+id/cardGraph"
            android:layout_width="match_parent"
            android:layout_height="400dp"
            motion:layout_constraintTop_toBottomOf="@id/table"

             />
    </ConstraintSet>
    <Transition
        motion:constraintSetStart="@+id/state_1_1"
        motion:constraintSetEnd="@+id/state_1_2" >
        <KeyFrameSet >
            <KeyAttribute
                motion:motionTarget="@+id/keyboardLayout1"
                motion:framePosition="70"
                android:alpha="0"
                android:scaleX="1.2" />
        </KeyFrameSet>
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/state_2_1"
        motion:constraintSetEnd="@+id/state_2_2" >

        <KeyFrameSet >
            <KeyAttribute
                motion:motionTarget="@+id/keyboardLayout1"
                motion:framePosition="70"
                android:alpha="0"
                android:scaleX="1.2" />
        </KeyFrameSet>
    </Transition>

</MotionScene>